<div class="container">
    <div class="small-10 large-centered columns">
      <table>
         <thead>
           <tr><th>Questions</th></tr>
         </thead>
        <tbody>
          <% @questions.each do |question| %>
         <tr>
           <td>
             <%= link_to question.title, question_path(question) %>
           </td>
         </tr>
        <% end %>
      </tbody>
      </table>
    </div>
  </div>
</div>
